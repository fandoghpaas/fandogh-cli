{
  "$id": "http://fandogh.cloud/service-manifest",
  "type": "array",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "items": {
    "$id": "http://fandogh.cloud/service-manifest/items",
    "type": "object",
    "properties": {
      "type": {
        "$id": "http://fandogh.cloud/service-manifest/items/properties/type",
        "type": "string",
        "title": "The Type Schema ",
        "default": "",
        "examples": [
          "managed-service"
        ]
      },
      "image": {
        "$id": "http://fandogh.cloud/service-manifest/items/properties/image",
        "type": "object",
        "properties": {
          "name": {
            "$id": "http://fandogh.cloud/service-manifest/items/properties/image/properties/name",
            "type": "string",
            "title": "The Name Schema ",
            "default": "",
            "examples": [
              "mysql"
            ]
          },
          "version": {
            "$id": "http://fandogh.cloud/service-manifest/items/properties/image/properties/version",
            "type": "string",
            "title": "The Version Schema ",
            "default": "",
            "examples": [
              "9.1"
            ]
          }
        }
      },
      "service_name": {
        "$id": "http://fandogh.cloud/service-manifest/items/properties/service_name",
        "type": "string",
        "title": "The Service_name Schema ",
        "default": "",
        "examples": [
          "mysql"
        ]
      },
      "parameters": {
        "$id": "http://fandogh.cloud/service-manifest/items/properties/parameters",
        "type": "array",
        "items": {
          "$id": "http://fandogh.cloud/service-manifest/items/properties/parameters/items",
          "type": "object",
          "properties": {
            "key": {
              "$id": "http://fandogh.cloud/service-manifest/items/properties/parameters/items/properties/key",
              "type": "string",
              "title": "The Key Schema ",
              "default": "",
              "examples": [
                "phpmyadmin_enabled"
              ]
            },
            "value": {
              "$id": "http://fandogh.cloud/service-manifest/items/properties/parameters/items/properties/value",
              "type": "boolean",
              "title": "The Value Schema ",
              "default": false,
              "examples": [
                true
              ]
            }
          }
        }
      }
    }
  }
}

